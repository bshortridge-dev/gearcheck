
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url = env("POSTGRES_PRISMA_URL") // uses connection pooling
  directUrl = env("POSTGRES_URL_NON_POOLING") // uses a direct connection
}

model Character {
  id            Int      @id @default(autoincrement())
  name          String
  link          String
  charRealm     String
  combinedData  String
  classSpec     String
  createdAt     DateTime @default(now())
  updatedAt     DateTime @updatedAt
}

model ArchonGear {
  id            Int      @id @default(autoincrement())
  categoryName  String
  itemName      String
  href          String
  maxKey        String
  popularity    String
  itemIcon      String
  classSpec     String
  createdAt     DateTime @default(now())
  updatedAt     DateTime @updatedAt
}